<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<configuration>
	<appender name="console" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d{HH:mm:ss.SSS} [%.8thread] %-5level %logger{24} - %msg%n</pattern>
		</encoder>
	</appender>

	<root level="info">
		<appender-ref ref="console"/>
	</root>
</configuration>
